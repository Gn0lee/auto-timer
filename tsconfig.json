{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@const/*": [
        "const/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@apps/*": [
        "apps/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    }
  },
  "include": [
    "src",
    ".expo/types/**/*.ts",
    "src/types/expo-env.d.ts",
    "src/types",
    "expo-env.d.ts"
  ],
  "exclude": [
    "babel.config.js"
  ]
}
